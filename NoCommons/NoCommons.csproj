<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net46;net45</TargetFrameworks>
    <AssemblyName>NoCommons</AssemblyName>
    <PackageId>NoCommons</PackageId>    
